{
  "editor.fontFamily": "BlexMono Nerd Font",
  "terminal.integrated.fontFamily": "BlexMono Nerd Font",
  "editor.fontLigatures": false,
  "editor.fontSize": 13,
  "omnisharp.useGlobalMono": "always",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "cpplint.cpplintPath": "/usr/bin/cpplint",
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.leader": ",",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [":"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.splitEditor"]
    },
    {
      "before": ["<leader>", "d"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["revealInExplorer"]
    },
    {
      "before": ["<leader>", "r", "f"],
      "commands": ["fileutils.renameFile"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": ["p"],
      "after": ["p", "g", "v", "y"]
    }
  ],
  "vim.statusBarColorControl": false,
  "vim.statusBarColors.normal": ["#262626", "#808080"],
  "vim.statusBarColors.insert": ["#BF616A", "#000"],
  "vim.statusBarColors.visual": ["#B48EAD", "#000"],
  "vim.statusBarColors.visualline": ["#B48EAD", "#000"],
  "vim.statusBarColors.visualblock": ["#A3BE8C", "#000"],
  "vim.statusBarColors.replace": "#D08770",
  "vim.statusBarColors.commandlineinprogress": "#007ACC",
  "vim.statusBarColors.searchinprogressmode": "#007ACC",
  "vim.statusBarColors.easymotionmode": "#007ACC",
  "vim.statusBarColors.easymotioninputmode": "#007ACC",
  "vim.statusBarColors.surroundinputmode": "#007ACC",
  "workbench.colorCustomizations": {
    "statusBar.background": "#262626",
    "statusBar.noFolderBackground": "#262626",
    "statusBar.debuggingBackground": "#262626",
    "statusBar.foreground": "#999999"
  },
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Community Material Theme Darker High Contrast",
  "window.menuBarVisibility": "toggle",
  "editor.lineNumbers": "relative",
  "vim.smartRelativeLine": true,
  "gitlens.modes": {
    "zen": {
      "name": "Zen",
      "statusBarItemName": "Zen",
      "description": "for a zen-like experience, disables many visual features",
      "codeLens": false,
      "currentLine": false,
      "hovers": false,
      "statusBar": false
    },
    "review": {
      "name": "Review",
      "statusBarItemName": "Reviewing",
      "description": "for reviewing code, enables many visual features",
      "codeLens": true,
      "currentLine": true,
      "hovers": true
    }
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "colorize.languages": [
    "javascript",
    "python",
    "json"
    // ...
  ],
  "colorize.files_extensions": [".json"],
  "colorize.ignore_files_extensions_deprecation": true,
  "editor.cursorStyle": "line-thin",
  "breadcrumbs.enabled": true,
  "workbench.statusBar.visible": true,
  "files.autoSaveDelay": 60000,
  "explorer.confirmDelete": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "java.refactor.renameFromFileExplorer": "autoApply",
  "gitlens.statusBar.alignment": "left",
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "terminal.integrated.rendererType": "dom",
  "[latex]": {
    "editor.defaultFormatter": "James-Yu.latex-workshop"
  },
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "C_Cpp.autocompleteAddParentheses": true,
  "C_Cpp.clang_format_style": "Google",
  "kite.showWelcomeNotificationOnStartup": false,
  "haskell.manageHLS": "GHCup",
  "window.zoomLevel": 1,
  "workbench.startupEditor": "none",
  "rubyLsp.formatter": "rubocop",
  "rubyLsp.rubyVersionManager": "none",
  "rubyLsp.enabledFeatures": {
    "codeActions": true,
    "diagnostics": true,
    "documentHighlights": true,
    "documentLink": true,
    "documentSymbols": true,
    "foldingRanges": true,
    "formatting": true,
    "hover": true,
    "inlayHint": true,
    "onTypeFormatting": true,
    "selectionRanges": true,
    "semanticHighlighting": true,
    "completion": true,
    "codeLens": true,
    "definition": true,
    "workspaceSymbol": true
  }
}
